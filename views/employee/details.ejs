<div id="f">
<div id="achievment">
    <h2>Achievment</h2>
    <hr style="width:50%;text-align:left;margin-left:0">
    <div>
        <li> <%= ApplicantInfo.achievement %></li>
        <input class="showuptade" type="text" id="achievement" name="achievement" value="<%= ApplicantInfo.achievement %>">
    </div>

</div>
</div>  
</div> 
<input class="dlt" id="showuptadebtn"  style= "width: 200px; position: absolute;" type="submit" value="Save">
</form>